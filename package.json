{
  "name": "bepswap-react-app",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "fblogin": "firebase login",
    "start": "node scripts/start.js",
    "build": "node scripts/build.js",
    "build:ci": "CI=true yarn build",
    "pretest": "env",
    "test": "yarn lint && yarn test:swagger && yarn test:unit && yarn test:feat",
    "test:feat:_no-build": "run-p -r cy:launch cy:run",
    "pretest:feat": "yarn build",
    "test:feat": "yarn test:feat:_no-build",
    "test:unit": "jest",
    "test:swagger": "node ./scripts/test-swagger",
    "cy:launch": "http-server build --silent",
    "cy:run": "./scripts/cypress-run",
    "cy:run:dev": "CYPRESS_baseUrl=http://localhost:3000 yarn cy:run --browser chrome",
    "cy:open": "cypress open",
    "cy:open:dev": "CYPRESS_baseUrl=http://localhost:3000 yarn cy:open",
    "deploy": "firebase deploy",
    "lint": "yarn lint:js && yarn lint:ts",
    "lint:js": "eslint 'src/**/*.{js,jsx}' --fix",
    "lint:ts": "eslint 'src/**/*.{ts,tsx}' --fix",
    "lint:watch": "esw --clear --watch --color src/**/*.js",
    "storybook": "start-storybook -p 6006",
    "build-storybook": "build-storybook -o ./build/storybook"
  },
  "dependencies": {
    "@binance-chain/javascript-sdk": "^2.16.2",
    "@ledgerhq/hw-transport-u2f": "^5.7.0",
    "@svgr/webpack": "5.1.0",
    "@trustwallet/walletconnect": "^0.1.37",
    "@walletconnect/qrcode-modal": "^1.0.0-beta.46",
    "antd": "^3.26.7",
    "axios": "^0.19.2",
    "axios-mock-adapter": "^1.17.0",
    "babel-jest": "^25.1.0",
    "babel-plugin-named-asset-import": "^0.3.5",
    "babel-plugin-styled-components": "^1.10.7",
    "babel-preset-react-app": "^9.1.0",
    "camelcase": "^5.3.1",
    "case-sensitive-paths-webpack-plugin": "2.3.0",
    "connected-react-router": "^6.6.1",
    "copy-to-clipboard": "^3.2.0",
    "css-loader": "3.4.2",
    "dotenv": "8.2.0",
    "dotenv-expand": "5.1.0",
    "eslint": "^6.8.0",
    "eslint-config-react-app": "^5.1.0",
    "eslint-loader": "3.0.3",
    "eslint-plugin-flowtype": "4.6.0",
    "eslint-plugin-react-hooks": "^2.3.0",
    "file-loader": "5.0.2",
    "fs-extra": "8.1.0",
    "html-webpack-plugin": "4.0.0-beta.5",
    "identity-obj-proxy": "3.0.0",
    "is-wsl": "^2.1.1",
    "jest": "25.1.0",
    "jest-environment-jsdom-fourteen": "1.0.1",
    "jest-resolve": "25.1.0",
    "jest-watch-typeahead": "0.4.2",
    "lodash": "^4.17.15",
    "mini-css-extract-plugin": "0.9.0",
    "node-sass": "^4.13.1",
    "nprogress": "^0.2.0",
    "optimize-css-assets-webpack-plugin": "5.0.3",
    "pnp-webpack-plugin": "1.5.0",
    "polished": "^3.4.3",
    "postcss-flexbugs-fixes": "4.1.0",
    "postcss-loader": "3.0.0",
    "postcss-normalize": "8.0.1",
    "postcss-preset-env": "6.7.0",
    "postcss-safe-parser": "4.0.1",
    "prop-types": "^15.7.2",
    "react": "^16.12.0",
    "react-app-polyfill": "^1.0.5",
    "react-circular-progressbar": "^2.0.3",
    "react-content-loader": "^5.0.0",
    "react-custom-scrollbars": "^4.2.1",
    "react-dev-utils": "^10.0.0",
    "react-dom": "^16.12.0",
    "react-draggable": "^4.2.0",
    "react-file-picker": "^0.0.6",
    "react-placeholder": "^3.0.2",
    "react-redux": "^7.1.3",
    "react-router-dom": "^5.1.2",
    "redux": "^4.0.5",
    "redux-devtools-extension": "^2.13.8",
    "redux-logger": "^3.0.6",
    "redux-saga": "^1.1.3",
    "redux-thunk": "^2.3.0",
    "resolve": "1.15.0",
    "sass-loader": "8.0.2",
    "semver": "7.1.1",
    "style-loader": "1.1.3",
    "styled-components": "^5.0.0",
    "styled-theme": "^0.3.3",
    "styled-tools": "^1.7.1",
    "terser-webpack-plugin": "2.3.2",
    "ts-pnp": "1.1.5",
    "url-loader": "3.0.0",
    "webpack": "4.41.5",
    "webpack-dev-server": "3.10.1",
    "webpack-manifest-plugin": "2.2.0",
    "workbox-webpack-plugin": "4.3.1"
  },
  "devDependencies": {
    "@babel/core": "^7.8.3",
    "@babel/plugin-proposal-nullish-coalescing-operator": "^7.8.3",
    "@babel/plugin-proposal-optional-chaining": "^7.8.3",
    "@storybook/addon-actions": "^5.3.9",
    "@storybook/addon-knobs": "^5.3.9",
    "@storybook/addon-links": "^5.3.9",
    "@storybook/addon-viewport": "^5.3.9",
    "@storybook/addons": "^5.3.9",
    "@storybook/react": "^5.3.9",
    "@testing-library/react-hooks": "^3.2.1",
    "@types/jest": "^24.9.1",
    "@types/react": "^16.9.19",
    "@types/react-dom": "^16.9.5",
    "@types/redux": "^3.6.0",
    "@typescript-eslint/eslint-plugin": "2.17.0",
    "@typescript-eslint/parser": "^2.17.0",
    "ajv": "^6.11.0",
    "babel-loader": "^8.0.6",
    "chalk": "^3.0.0",
    "cypress": "^3.8.3",
    "cypress-failed-log": "^2.6.0",
    "cypress-file-upload": "^3.5.3",
    "enzyme": "^3.11.0",
    "enzyme-adapter-react-16": "^1.15.2",
    "enzyme-to-json": "^3.4.3",
    "eslint-config-airbnb": "^18.0.1",
    "eslint-config-prettier": "^6.9.0",
    "eslint-plugin-css-modules": "^2.11.0",
    "eslint-plugin-import": "^2.20.0",
    "eslint-plugin-jsx-a11y": "^6.2.3",
    "eslint-plugin-react": "^7.18.0",
    "eslint-watch": "^6.0.1",
    "glob-promise": "^3.4.0",
    "http-server": "^0.12.1",
    "husky": "^4.2.1",
    "json-schema-ref-parser": "^7.1.3",
    "lint-staged": "^10.0.2",
    "npm-run-all": "^4.1.5",
    "prettier": "^1.19.1",
    "react-test-renderer": "^16.12.0",
    "tslint-config-prettier": "^1.18.0",
    "typescript": "^3.7.5"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged",
      "pre-push": "yarn lint"
    }
  },
  "lint-staged": {
    "src/**/*.{js,jsx,json,css}": [
      "prettier --single-quote --trailing-comma all --write",
      "git add"
    ]
  },
  "eslintConfig": {
    "extends": "react-app"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "jest": {
    "collectCoverageFrom": [
      "src/**/*.{js,jsx,ts,tsx}",
      "!src/**/*.d.ts"
    ],
    "setupFiles": [
      "react-app-polyfill/jsdom"
    ],
    "setupFilesAfterEnv": [
      "<rootDir>/src/setupTests.js"
    ],
    "testMatch": [
      "<rootDir>/src/**/__tests__/**/*.{js,jsx,ts,tsx}",
      "<rootDir>/src/**/*.{spec,test}.{js,jsx,ts,tsx}"
    ],
    "testEnvironment": "jest-environment-jsdom-fourteen",
    "transform": {
      "^.+\\.(js|jsx|ts|tsx)$": "<rootDir>/node_modules/babel-jest",
      "^.+\\.css$": "<rootDir>/config/jest/cssTransform.js",
      "^(?!.*\\.(js|jsx|ts|tsx|css|json)$)": "<rootDir>/config/jest/fileTransform.js"
    },
    "transformIgnorePatterns": [
      "[/\\\\]node_modules[/\\\\].+\\.(js|jsx|ts|tsx)$",
      "^.+\\.module\\.(css|sass|scss)$"
    ],
    "modulePaths": [],
    "moduleNameMapper": {
      "^react-native$": "react-native-web",
      "^.+\\.module\\.(css|sass|scss)$": "identity-obj-proxy"
    },
    "moduleFileExtensions": [
      "web.js",
      "js",
      "web.ts",
      "ts",
      "web.tsx",
      "tsx",
      "json",
      "web.jsx",
      "jsx",
      "node"
    ],
    "watchPlugins": [
      "jest-watch-typeahead/filename",
      "jest-watch-typeahead/testname"
    ]
  }
}
